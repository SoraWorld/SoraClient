<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.net.URL?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" fx:id="mainFrame" focusTraversable="true"
            onMouseDragged="#WindowDragged" onMousePressed="#WindowPressed" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/8.0.60" fx:controller="org.soraworld.soraclient.ui.MainFrame">
    <top>
        <AnchorPane id="topbar" prefHeight="44.0" styleClass="icon" BorderPane.alignment="CENTER">
            <BorderPane.margin>
                <Insets/>
            </BorderPane.margin>
            <ImageView fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true"
                       AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="5.0" AnchorPane.topAnchor="5.0">
                <Image url="@../images/icon.png"/>
            </ImageView>
            <HBox layoutX="461.0" layoutY="10.0" spacing="5.0" AnchorPane.rightAnchor="5.0">
                <Button mnemonicParsing="false" onMouseClicked="#WindowMinimize" styleClass="icon" text="%icon.mini"/>
                <Button disable="true" mnemonicParsing="false" text="%icon.tray"/>
                <Button mnemonicParsing="false" onMouseClicked="#WindowClose" styleClass="icon" text="%icon.close"/>
            </HBox>
            <Text boundsType="VISUAL" cache="true" fill="WHITE" layoutX="180.0" layoutY="32.0" styleClass="title"
                  text="空の境界MC"/>
            <Text fx:id="title_version" boundsType="VISUAL" cache="true" fill="WHITE" layoutX="320.0" layoutY="32.0"
                  styleClass="title"/>
        </AnchorPane>
    </top>
    <left>
        <AnchorPane focusTraversable="true" prefHeight="356.0" prefWidth="172.0" BorderPane.alignment="CENTER">
            <VBox focusTraversable="true" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                  AnchorPane.topAnchor="0.0">
                <cursor>
                    <Cursor fx:constant="HAND"/>
                </cursor>
                <AnchorPane styleClass="left-row">
                    <RadioButton fx:id="left_game" contentDisplay="CENTER" layoutX="7.0" mnemonicParsing="false"
                                 onMouseClicked="#LeftMenuClicked" selected="true" styleClass="left-menu" text="空 之 境 界"
                                 AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                                 AnchorPane.topAnchor="0.0">
                        <textFill>
                            <LinearGradient>
                                <stops>
                                    <Stop color="#bf1515"/>
                                    <Stop color="#2356f2" offset="1.0"/>
                                </stops>
                            </LinearGradient>
                        </textFill>
                        <toggleGroup>
                            <ToggleGroup fx:id="leftmenu"/>
                        </toggleGroup>
                        <font>
                            <Font size="14.0"/>
                        </font>
                        <opaqueInsets>
                            <Insets/>
                        </opaqueInsets>
                    </RadioButton>
                    <Label layoutX="14.0" layoutY="13.0" text="%icon.soraworld" AnchorPane.bottomAnchor="0.0"
                           AnchorPane.leftAnchor="16.0" AnchorPane.topAnchor="0.0">
                        <styleClass>
                            <String fx:value="icon"/>
                            <String fx:value="icon-mid"/>
                        </styleClass>
                    </Label>
                </AnchorPane>
                <AnchorPane styleClass="left-row">
                    <RadioButton fx:id="left_setting" contentDisplay="CENTER" layoutX="7.0" mnemonicParsing="false"
                                 onMouseClicked="#LeftMenuClicked" styleClass="left-menu" text="游 戏 设 置"
                                 toggleGroup="$leftmenu" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                 AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                        <textFill>
                            <LinearGradient>
                                <stops>
                                    <Stop color="#bf1515"/>
                                    <Stop color="#2356f2" offset="1.0"/>
                                </stops>
                            </LinearGradient>
                        </textFill>
                        <font>
                            <Font size="14.0"/>
                        </font>
                        <opaqueInsets>
                            <Insets/>
                        </opaqueInsets>
                    </RadioButton>
                    <Label layoutX="14.0" layoutY="13.0" text="%icon.game" AnchorPane.bottomAnchor="0.0"
                           AnchorPane.leftAnchor="16.0" AnchorPane.topAnchor="0.0">
                        <styleClass>
                            <String fx:value="icon"/>
                            <String fx:value="icon-mid"/>
                        </styleClass>
                    </Label>
                </AnchorPane>
                <AnchorPane styleClass="left-row">
                    <RadioButton fx:id="left_system" contentDisplay="CENTER" layoutX="7.0" mnemonicParsing="false"
                                 onMouseClicked="#LeftMenuClicked" styleClass="left-menu" text="系 统 设 置"
                                 toggleGroup="$leftmenu" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                 AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                        <textFill>
                            <LinearGradient>
                                <stops>
                                    <Stop color="#bf1515"/>
                                    <Stop color="#2356f2" offset="1.0"/>
                                </stops>
                            </LinearGradient>
                        </textFill>
                        <font>
                            <Font size="14.0"/>
                        </font>
